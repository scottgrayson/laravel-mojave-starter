{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "artesaos/seotools": "^0.11.0",
        "braintree/braintree_php": "^3.25",
        "doctrine/dbal": "^2.6",
        "erusev/parsedown": "^1.6",
        "fideloper/proxy": "~4.0",
        "fzaninotto/faker": "~1.4",
        "gloudemans/shoppingcart": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "imangazaliev/didom": "^1.12",
        "lab404/laravel-impersonate": "^1.2",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/csv": "^9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/html-to-markdown": "^4.6",
        "nothingworks/blade-svg": "^0.2.2",
        "pda/pheanstalk": "^3.1",
        "propaganistas/laravel-phone": "^3.0",
        "pusher/pusher-php-server": "^3.0",
        "ramsey/uuid": "^3.5",
        "rollbar/rollbar-laravel": "^2.2",
        "spatie/laravel-backup": "^5.1",
        "spatie/laravel-permission": "^2.5",
        "spatie/laravel-sitemap": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "codedungeon/phpunit-result-printer": "^0.4.4",
        "filp/whoops": "~2.0",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "wnx/laravel-stats": "^1.0",
        "nunomaduro/collision": "~1.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "rollbar/rollbar-laravel"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/publiux/LaravelShoppingcart-1"
        }
    ]
}
